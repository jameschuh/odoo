        <record id="estate_model_offer_view_tree" model="ir.ui.view">
            <field name="name">estate.property.offer.tree</field>
            <field name="model">estate.property.offer</field>
            <field name="arch" type="xml">
                <tree string="Estate Offer Tree">
                    <field name="partner_id"/>
                    <field name="price"/>
                    <field name="validity"/>
                    <field name="date_deadline"/>
                    <field name="status"/>
                </tree>
            </field>
        </record>
        <record id="estate_model_view_offer_form" model="ir.ui.view">
            <field name="name">estate.property.offer.form</field>
            <field name="model">estate.property.offer</field>
            <field name="arch" type="xml">
                <form string="Offer Form">
                    <sheet>
                        <group>
                            <group>
                                <field name="partner_id"/>
                                <field name="price"/>
                                <field name="validity"/>
                                <field name="date_deadline"/>
                                <field name="status"/>
                            </group>
                        </group>
                    </sheet>
                </form>
            </field>
        </record>
</odoo>
